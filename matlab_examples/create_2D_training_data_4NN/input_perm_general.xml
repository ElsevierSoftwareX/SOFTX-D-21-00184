<?xml version="1.0" ?>

<geometry>
    <!-- The name of the .dat file that contains the entire geometry-->
    <file_geom> X_data_4NN_258_128_3 </file_geom>
    <!-- Size (in voxels) of the simulation domain. -->
    <size> <x> 258 </x> <y> 128 </y> <z> 3 </z> </size>
	<!-- Turn periodicty TRUE or FALSE in x,y,z. -->
	<per> <x> false </x> <y> false </y> <z> true </z> </per>
</geometry>

<folder>
    <!-- Output folder -->
    <out_f> lbm_results/X/ </out_f>
    <!-- Input folder -->
    <in_f> input/ </in_f>
</folder>

<simulations>
	<!-- Total number of geometries / individual simulations -->
	<num> 1 </num>
	<!-- Pressure-->
    <press> 0.0005 </press>
	<!-- Max iterations-->
    <iter> 10000000 </iter>
	<!-- Convergence-->
    <conv> 0.001 </conv>
	<!-- Save vtks for medium and velocity-->
    <vtk_out> true </vtk_out>
</simulations>
